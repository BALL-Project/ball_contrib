MESSAGE(STATUS "Installing fftw3")

FILE(GLOB_RECURSE FFTW3_LIBS "@BUILDDIR@/@DOLLAR@{config}" "*.lib")
FILE(GLOB_RECURSE FFTW3_DLLS "@BUILDDIR@/@DOLLAR@{config}" "*.dll")
FILE(GLOB_RECURSE FFTW3_PDBS "@BUILDDIR@/@DOLLAR@{config}" "*.pdb")

FILE(INSTALL @DOLLAR@{TBB_LIBS} DESTINATION "@CONTRIB_INSTALL_LIB@")
FILE(INSTALL @DOLLAR@{TBB_DLLS} DESTINATION "@CONTRIB_INSTALL_BIN@")
FILE(INSTALL @DOLLAR@{TBB_PDBS} DESTINATION "@CONTRIB_INSTALL_LIB@")

FILE(INSTALL "@PROJECT_BINARY_DIR@/src/fftw3/api/fftw3.h" DESTINATION "@CONTRIB_INSTALL_INC@")
